{
  "name": "@mention/spaces",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --reset-cache --clear",
    "dev": "expo start --reset-cache --tunnel --clear",
    "dev:local": "expo start --reset-cache --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo export --platform web --output-dir dist",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "clean": "rm -rf dist .expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@livekit/react-native": "^2.9.6",
    "@livekit/react-native-expo-plugin": "^1.0.1",
    "@livekit/react-native-webrtc": "^137.0.2",
    "@mention/spaces-shared": "*",
    "@oxyhq/core": "1.6.1",
    "@oxyhq/services": "6.3.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@tanstack/react-query": "^5.90.6",
    "axios": "^1.7.9",
    "expo": "~54.0.25",
    "expo-audio": "~1.1.1",
    "expo-build-properties": "~1.0.10",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.9",
    "livekit-client": "^2.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.21.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "sonner-native": "^0.21.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
